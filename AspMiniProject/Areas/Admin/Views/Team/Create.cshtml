@model TeamCreateVM

<div class="container-fluid mt-4">
    <form class="forms-sample" asp-action="Create" method="post" enctype="multipart/form-data">
        <div class="form-group">
                <label for="slider">Image</label>
                <input asp-for="Image" class="form-control" id="slider" maxlength="1000">
                <span class="text-danger" asp-validation-for="Image"></span>
      

            <label for="sliderinfo">FullName</label>
            <input asp-for="FullName" class="form-control" id="sliderinfo" maxlength="1000">
            <span class="text-danger" asp-validation-for="FullName"></span>


            <label for="sliderinfo">Position</label>
            <input asp-for="Position" class="form-control" id="sliderinfo" maxlength="1000">
            <span class="text-danger" asp-validation-for="Position"></span>
        </div>
        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <a asp-action="Index" class="btn btn-light">Cancel</a>
    </form>
</div>